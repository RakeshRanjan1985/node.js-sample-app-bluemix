extends layout

block content
    h1= title
    p Registration Details

    // Wrapper
    #wrapper

        // USER INFO
        #userInfo
            h2 User Info
            p
                strong Name:
                |  <span id='userInfoName'></span>
                br
                strong Email-Id:
                |  <span id='userInfoEmail'></span>
                br
                strong Organisation:
                |  <span id='userInfoOrganization'></span>
                br
                strong Comment:
                |  <span id='userInfoComments'></span>
        // /USER INFO

        // USER LIST
        h2 User List
        #userList
            table
                thead
                    th UserName
                    th Email
                    th Company
                tbody
        // /USER LIST

    // /WRAPPER